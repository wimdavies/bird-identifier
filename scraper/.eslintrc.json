{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-await-in-loop": 0,
        "no-constant-condition": 0,
        "import/extensions": 0,
        "no-restricted-syntax": 0,
        "consistent-return": 0
    }
}
